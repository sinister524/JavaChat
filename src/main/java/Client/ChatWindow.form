<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="Client.ChatWindow">
  <grid id="27dc6" binding="rootPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="d2f78" binding="centralPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <scrollpane id="19f50" binding="chatScroll">
            <constraints border-constraint="Center"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="12770" class="javax.swing.JTextArea" binding="chatMessages">
                <constraints/>
                <properties>
                  <editable value="false"/>
                  <lineWrap value="true"/>
                  <wrapStyleWord value="true"/>
                </properties>
              </component>
            </children>
          </scrollpane>
        </children>
      </grid>
      <grid id="93e8" binding="downPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="1e6bf" class="javax.swing.JButton" binding="sendButton">
            <constraints border-constraint="East"/>
            <properties>
              <enabled value="true"/>
              <preferredSize width="100" height="60"/>
              <text value="Отправить"/>
            </properties>
          </component>
          <component id="39e6a" class="javax.swing.JTextField" binding="messageField">
            <constraints border-constraint="Center"/>
            <properties/>
          </component>
        </children>
      </grid>
      <grid id="d3fbf" binding="rightPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="East"/>
        <properties>
          <minimumSize width="92" height="15"/>
        </properties>
        <border type="none"/>
        <children>
          <scrollpane id="e0e58" binding="clientList">
            <constraints border-constraint="Center"/>
            <properties>
              <maximumSize width="100" height="32767"/>
              <minimumSize width="100" height="15"/>
              <preferredSize width="100" height="0"/>
            </properties>
            <border type="none"/>
            <children/>
          </scrollpane>
        </children>
      </grid>
      <grid id="dc8fd" binding="upPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="e1889" class="javax.swing.JLabel" binding="chatLabel">
            <constraints border-constraint="Center"/>
            <properties>
              <focusCycleRoot value="false"/>
              <focusTraversalPolicyProvider value="false"/>
              <text value="Чат:"/>
              <verticalAlignment value="0"/>
              <verticalTextPosition value="0"/>
            </properties>
          </component>
          <component id="f24e" class="javax.swing.JLabel" binding="onlineNow">
            <constraints border-constraint="East"/>
            <properties>
              <preferredSize width="92" height="32"/>
              <text value=""/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
